//updateContactsByQueueble newQue = new updateContactsByQueueble ();     
//System.enqueueJob(newQue);
public class updateContactsByQueueble implements Queueable {
    public void execute(QueueableContext context) {
        List<Account> accounts = [SELECT Id, 
                                         BillingStreet, 
                                         BillingCity, 
                                         BillingState, 
                                         BillingCountry, 
                                         BillingPostalCode FROM Account LIMIT 150];
        List<Contact> contacts = new List<Contact>();
        Set<Id> accountIds = new Set<Id>();
        for (Account account : accounts){
        	if ((!string.isBlank(account.BillingStreet)) ||
                (!string.isEmpty(account.BillingCity)) ||
                (!string.isEmpty(account.BillingState)) || 
                (!string.isEmpty(account.BillingCountry)) || 
                (!string.isEmpty(account.BillingPostalCode)))
            {
                accountIds.add(account.Id);
            }
        }
        contacts = [SELECT Id FROM Contact WHERE AccountId IN : accountIds];
        for (Contact contact : contacts){
        	contact.Is_Synced__c = false;
            contact.Processed_By_Queue__c = true;
        }
  		update contacts;	        
 	   }
}
