    public static void OnAfterUpdate(List<Account> newList, List<Account> oldList,Map<Id,Account> newMap,Map<Id,Account> oldMap){
        SingletonTriggerHelper.addDateToName (newList);   
    }

    public static void OnBeforeDelete(List<Account> oldList, Map<Id,Account> oldMap){
        
    }

    public static void OnAfterDelete(List<Account> deletedAccounts, Map <Id,Account> deltedMap){
        
    }

    public static void OnUndelete(List<Account> restoredAccounts){
        
    }
}
